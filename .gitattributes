* text=auto
# Notebooks + JSON use LF
*.ipynb text eol=lf
*.json  text eol=lf
# Code files use LF
*.py text eol=lf
*.sh text eol=lf
*.md text eol=lf
# Windows scripts use CRLF
*.bat text eol=crlf
*.cmd text eol=crlf
*.joblib filter=lfs diff=lfs merge=lfs -text
*.keras filter=lfs diff=lfs merge=lfs -text
*.tif filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text
*.gif filter=lfs diff=lfs merge=lfs -text
models/anyburn_rf_best.joblib filter=lfs diff=lfs merge=lfs -text
